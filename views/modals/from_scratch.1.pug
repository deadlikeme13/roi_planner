.container
  // Input Info from Scratch Modal
  #fromScratchModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='fromScratchModal' aria-hidden='true')
    .modal-lg.modal-dialog.modal-dialog-scrollable(role='document')
      .modal-content
        .modal-header.nav.nav-tabs.row.d-flex.justify-content-between.mx-1.mx-sm-3.mb-0.pb-0.border-0
          #tab01.tabs.active
            h6.font-weight-bold Mortgage Info
          #tab02.tabs
            h6.text-muted Annual Expenses
          #tab03.tabs
            h6.text-muted Refinance
          #tab04.tabs
            h6.text-muted Improvements
          #tab05.tabs
            h6.text-muted Selling Costs
        .line
        .modal-body.p-0
          fieldset#tab011.show
            .bg-light
              form.pt-3.pb-3
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='house-price') House Price
                    input.form-control(min='1', id='house-price', type='number')
                  .col-6
                    label(for='downpayment') Downpayment
                    input.form-control(min='0', id='downpayment', type='number')
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='interest') Interest Rate (%)
                    input.form-control(min='0', max='6', step='0.01', id='interest', type='number')
                  .col-6
                    label(for='term') Term (Yrs)
                    input.form-control(min='1', max='99', id='term', type='number')
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='pay-start') Start Date
                    input.form-control(id='pay-start', type='date')
                  .col-6
                    label(for='pay-frequency') Payment Frequency
                    select.custom-select
                      option(selected='', disabled) Choose One
                      option(value='weekly') Weekly
                      option(value='bi-weekly') Bi-Weekly
                      option(value='monthly') Monthly
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='step') Periodicity (Months)
                    input.form-control(min='1', max='12', id='step', type='number')
                  .col-6
                    label(for='fees') Financing Fees
                    input.form-control(min='1', max='12', id='fees', type='number')
                .form-group.pb-5.px-3.row.justify-content-center
                  button.btn.btn-secondary(type='button' disabled) Additional Payments +
                .form-group.row.modal-body-footer
                  button.active.btn.btn-primary.col-md-2.px-3(type='button' disabled) Previous
                  #tab02.tabs.active
                    h6
                      button.btn.btn-primary.col-md-2.ml-auto.px-3(type='button') Next
                  //i.fa.fa-angle-double-left.px-3
                  //a(href='#tab021' data-toggle='tab' data-target='#tab02')  
                  //i.fa.fa-angle-double-right.ml-auto.px-3
          fieldset#tab021
            .bg-light
              form.pt-3.pb-3
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='tax') Property Tax
                    input.form-control(min='1', id='tax', type='number')
                  .col-6
                    label(for='insurance') Property Insurance
                    input.form-control(min='1', id='insurance', type='number')
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='appreciation') Property Appreciation
                    input.form-control(min='0', max='6', step='0.01', id='appreciation', type='number')
                  .col-6
                    label(for='tax-rate') Tax Rate
                    input.form-control(min='0', max='6', step='0.01', id='tax-rate', type='number')
            .form-group.row.modal-body-footer
              button.tabs.btn.btn-primary.col-md-2.px-3(type='button') Previous
              button.tabs.btn.btn-primary.col-md-2.ml-auto.px-3(type='button') Next
          fieldset#tab031
            .bg-light
              form.pt-3.pb-3
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='refinance-name') Label
                    input.form-control(min='1', max='20', id='refinance-name', type='text')
                  .col-6
                    label(for='start-balance') Starting Balance
                    input.form-control(min='1', id='start-balance', type='number')
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='refinance-interest') Interest Rate (%)
                    input.form-control(min='0', max='6', step='0.01', id='refinance-interest', type='number')
                  .col-6
                    label(for='refinance-term') Term (Yrs)
                    input.form-control(min='1', max='99', id='refinance-term', type='number')
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='refinance-pay-start') Start Date
                    input.form-control(id='refinance-pay-start', type='date')
                  .col-6
                    label(for='refinance-pay-frequency') Payment Frequency
                    select.custom-select
                      option(selected='', disabled) Choose One
                      option(value='weekly') Weekly
                      option(value='bi-weekly') Bi-Weekly
                      option(value='monthly') Monthly
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='refinance-step') Periodicity (Months)
                    input.form-control(min='1', max='12', id='refinance-step', type='number')
                  .col-6
                    label(for='refinance-fees') Financing Fees
                    input.form-control(min='1', max='12', id='refinance-fees', type='number')
                .form-group.pb-5.px-3.row.justify-content-center
                  button.btn.btn-secondary(type='button' disabled) Additional Payments +
                .form-group.pb-5.row.justify-content-center
                  button.btn.btn-primary.px-3(type='button') Save
                .form-group.row.pb-5.px-3.justify-content-center
                  button.btn.btn-secondary(type='button' disabled) Add Refinance +
                .form-group.row.modal-body-footer
                  button.btn.btn-primary.col-md-2.px-3.previous-tab(type='button') Previous
                  button.btn.btn-primary.col-md-2.ml-auto.px-3.next-tab(type='button') Next
          fieldset#tab041
            .bg-light
              form.pt-3.pb-3
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='improvement-name') Label
                    input.form-control(min='1', max='20', id='improvement-name', type='text')
                  .col-6
                    label(for='cost') Cost
                    input.form-control(min='1', id='cost', type='number')
                .form-group.pb-5.px-3.row.justify-content-center
                  button.btn.btn-secondary(type='button' disabled) Add Improvement +
            .px-3
              .border.border-1.box
                table.table.table-hover
                  thead.thead-light
                    tr
                      th(scope='col') Label
                      th(scope='col') Cost
                  tbody
                    tr
                      td Deck
                      td 10000
                    tr
                      td Roof
                      td 18000
                  tfoot
                    tr
                      td 
                        strong Total:
                      td $28000
            .form-group.row.modal-body-footer
              button.btn.btn-primary.col-md-2.px-3(type='button') Previous
              button.btn.btn-primary.col-md-2.ml-auto.px-3(type='button') Next
          fieldset#tab051
            .bg-light
              form.pt-3.pb-3
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='commission') Realtor Commission
                    input.form-control(min='0', max='6', step='0.01', id='commission', type='number')
                  .col-6
                    label(for='legal') Legal Disbursements &amp; Fees
                    input.form-control(min='0', id='legal', type='number')
                .form-group.row.pb-2.px-3
                  .col-6
                    label(for='discharge-fees') Discharge Fees
                    input.form-control(min='0', id='discharge-fees', type='number')
                  .col-6
                    label(for='transfer-tax') Transfer Tax
                    input.form-control(min='0', id='transfer-tax', type='number')
            .form-group.row.modal-body-footer
              button.btn.btn-primary.col-md-2.px-3(type='button') Previous
              button.btn.btn-primary.col-md-3.ml-auto.px-3(type='button' disabled) Create Schedule
        .line
        .modal-footer.d-flex.flex-column.justify-content-center.border-0
          p.text-muted Can&apos;t find what you&apos;re looking for?
          button.btn.btn-secondary(type='button') Contact Support Team